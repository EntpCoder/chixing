<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songyang.mapper.EmpMapper">
    <select id="selectAll" resultType="com.songyang.entity.Emp">
        select *
        from emp
    </select>
    <select id="selectById" resultType="emp">
        select *
        from emp
        where empno = #{id}
    </select>
    <insert id="insert">
        insert into emp
        values (#{empno}, #{ename}, #{job}, #{mgr}, #{hiredate}, #{salary}, #{comm}, #{deptno})
    </insert>
</mapper>